{
  "name": "dotnet.ts.react.redux",
  "version": "0.0.1-alpha",
  "description": "dotnet.ts.react.redux template project",
  "main": "dist/main.js.js",
  "scripts": {
    "lint": "tslint -e **/node_modules/** -e **/dist/** -e **/typings/** -t codeFrame **/*.ts ",
    "build": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --env=prod -p --progress --profile --colors",
    "build-lib": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --env=prod --config ./webpack-lib.production.config.js -p --progress --profile --colors",
    "build:dev": "webpack --progress --profile --colors",
    "prepush": "npm run lint && npm run build",
    "prepush-lib": "npm run lint && npm run build-lib",
    "publish-prod": "npm run prepush-lib && npm pack",
    "build-dev": "cross-env NODE_ENV=development node --trace-warnings --trace-deprecation -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors",
    "build-prod": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --env=prod --progress --profile --colors --display-error-details",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.dll.config.js --progress --profile --colors",
    "build-dll-dev": "npm run build-dll && npm run build-dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "typings": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arragro/ArragroCMS.git"
  },
  "keywords": [
    "dotnet",
    "core",
    "cms"
  ],
  "author": "Michael Jewell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arragro/dotnet.ts.react.redux/issues"
  },
  "homepage": "https://github.com/Arragro/dotnet.ts.react.redux#readme",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "domain-task": "^3.0.3",
    "formsy-react": "^1.1.4",
    "formsy-react-components": "^1.0.0",
    "js-cookie": "^2.2.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-s-alert": "^1.4.1",
    "reactstrap": "^5.0.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/js-cookie": "^2.1.0",
    "@types/node": "9.4.6",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.12",
    "@types/react-s-alert": "^1.3.1",
    "@types/reactstrap": "^5.0.20",
    "@types/redux-actions": "2.2.4",
    "@types/webpack": "^3.0.14",
    "@types/webpack-env": "^1.13.5",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.3",
    "aspnet-webpack-react": "^3.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.10",
    "concurrently": "^3.5.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "^3.1.3",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.1",
